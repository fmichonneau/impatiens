journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Animals,Anthozoa,Anthozoa: classification,Anthozoa: physiology,Coral Reefs,Ecological and Environmental Processes,Ecosystem,Environment,Geological Phenomena,Pacific Ocean,Species Specificity},
month = jul,
number = {1763},
pages = {20130818},
pmid = {23698011},
title = {{Faunal breaks and species composition of Indo-Pacific corals: the role of plate tectonics, environment and habitat distribution.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23698011},
volume = {280},
