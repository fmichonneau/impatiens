keywords = {Animal Feed,Animals,Base Sequence,Butterflies,Butterflies: classification,Butterflies: genetics,Butterflies: growth \& development,DNA Primers,Molecular Sequence Data,Phylogeny,Plants,Species Specificity,Tropical Climate},
month = oct,
number = {41},
pages = {14812--7},
pmid = {15465915},
title = {{Ten species in one: DNA barcoding reveals cryptic species in the neotropical skipper butterfly Astraptes fulgerator.}},
