journal = {Molecular phylogenetics and evolution},
keywords = {Amino Acid Sequence,Animals,Base Sequence,DNA,Electron Transport Complex IV,Electron Transport Complex IV: genetics,Genetic Variation,Mitochondrial,Mitochondrial: genetics,Molecular Sequence Data,Mutation,Phylogeny,Polymerase Chain Reaction,RNA,Ribosomal,Ribosomal: genetics,Sea Cucumbers,Sea Cucumbers: classification,Sea Cucumbers: genetics,Sequence Alignment},
month = dec,
number = {3},
pages = {425--37},
pmid = {8975697},
title = {{Molecular phylogeny of eastern Pacific sea cucumbers (Echinodermata: Holothuroidea) based on mitochondrial DNA sequence.}},
volume = {6},
