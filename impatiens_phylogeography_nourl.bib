journal = {Molecular ecology resources},
keywords = {2009,accepted 20 december 2009,coi amplification,echinoderms,identification,mitochondrial dna,primer design,rapid species,received 9 september 2009,revision received 29 november,universal markers},
month = nov,
number = {6},
pages = {960--7},
