@article{Leache2010,
abstract = {Genealogical data are an important source of evidence for delimiting species, yet few statistical methods are available for calculating the probabilities associated with different species delimitations. Bayesian species delimitation uses reversible-jump Markov chain Monte Carlo (rjMCMC) in conjunction with a user-specified guide tree to estimate the posterior distribution for species delimitation models containing different numbers of species. We apply Bayesian species delimitation to investigate the speciation history of forest geckos (Hemidactylus fasciatus) from tropical West Africa using five nuclear loci (and mtDNA) for 51 specimens representing 10 populations. We find that species diversity in H. fasciatus is currently underestimated, and describe three new species to reflect the most conservative estimate for the number of species in this complex. We examine the impact of the guide tree, and the prior distributions on ancestral population sizes () and root age ($\tau$(0)), on the posterior probabilities for species delimitation. Mis-specification of the guide tree or the prior distribution for can result in strong support for models containing more species. We describe a new statistic for summarizing the posterior distribution of species delimitation models, called speciation probabilities, which summarize the posterior support for each speciation event on the starting guide tree.},
author = {Leach\'{e}, Adam D and Fujita, Matthew K},
doi = {10.1098/rspb.2010.0662},
file = {:home/francois/Library/Articles/Leache+Fujita2010.pdf:pdf},
issn = {1471-2954},
journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Animals,Bayes Theorem,Biodiversity,Genetic Speciation,Lizards,Lizards: physiology,Markov Chains,Monte Carlo Method,Population Density,Population Dynamics},
month = oct,
number = {1697},
pages = {3071--7},
pmid = {20519219},
title = {{Bayesian species delimitation in West African forest geckos (Hemidactylus fasciatus).}},
volume = {277},
year = {2010}
}
