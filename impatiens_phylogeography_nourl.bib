journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Animals,Bayes Theorem,Biological Evolution,Cell Nucleus,Cell Nucleus: genetics,DNA, Mitochondrial,DNA, Mitochondrial: genetics,Electron Transport Complex IV,Electron Transport Complex IV: genetics,Genetic Variation,Introns,Microsatellite Repeats,Molecular Sequence Data,Phylogeography,Polymerase Chain Reaction,Queensland,RNA, Transfer,RNA, Transfer: genetics,Reproduction,Starfish,Starfish: genetics,Starfish: physiology},
month = oct,
number = {1744},
